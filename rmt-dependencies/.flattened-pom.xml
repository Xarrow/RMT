<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>io.github.xarrow.rmt</groupId>
    <artifactId>rmt</artifactId>
    <version>1.0</version>
  </parent>
  <groupId>io.github.xarrow.rmt</groupId>
  <artifactId>rmt-dependencies</artifactId>
  <version>1.0</version>
  <packaging>pom</packaging>
  <licenses>
    <license>
      <name>The Apache License, Version 2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
    </license>
  </licenses>
  <properties>
    <java.encoding>utf-8</java.encoding>
    <lombok.version>1.18.12</lombok.version>
    <maven-compiler-plugin.version>3.8.0</maven-compiler-plugin.version>
    <spring-boot.version>2.2.6.RELEASE</spring-boot.version>
    <jna.version>4.5.0</jna.version>
    <commons-io.version>2.7</commons-io.version>
    <maven-resources-plugin.version>3.0.1</maven-resources-plugin.version>
    <commons.version>3.4</commons.version>
    <maven.compiler.source>1.8</maven.compiler.source>
    <compiler.version>1.8</compiler.version>
    <java.version>1.8</java.version>
    <compiler.encoding>utf-8</compiler.encoding>
    <project.encoding>utf-8</project.encoding>
    <maven.compiler.target>1.8</maven.compiler.target>
    <maven-surefire-plugin.version>2.22.0</maven-surefire-plugin.version>
    <consul-client.version>1.4.0</consul-client.version>
    <pty4j.version>0.10.0</pty4j.version>
    <commons-codec.version>1.10</commons-codec.version>
    <maven-antrun-plugin.version>1.3</maven-antrun-plugin.version>
  </properties>
  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>io.github.xarrow.rmt</groupId>
        <artifactId>rmt-commons</artifactId>
        <version>${project.version}</version>
      </dependency>
      <dependency>
        <groupId>io.github.xarrow.rmt</groupId>
        <artifactId>rmt-api</artifactId>
        <version>${project.version}</version>
      </dependency>
      <dependency>
        <groupId>io.github.xarrow.rmt</groupId>
        <artifactId>rmt-examples</artifactId>
        <version>${project.version}</version>
      </dependency>
      <dependency>
        <groupId>io.github.xarrow.rmt</groupId>
        <artifactId>rmt-expand</artifactId>
        <version>${project.version}</version>
      </dependency>
      <dependency>
        <groupId>io.github.xarrow.rmt</groupId>
        <artifactId>rmt-app</artifactId>
        <version>${project.version}</version>
      </dependency>
      <dependency>
        <groupId>io.github.xarrow.rmt</groupId>
        <artifactId>rmt-spring-boot-starter</artifactId>
        <version>${project.version}</version>
      </dependency>
      <dependency>
        <groupId>commons-io</groupId>
        <artifactId>commons-io</artifactId>
        <version>${commons-io.version}</version>
      </dependency>
      <dependency>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-dependencies</artifactId>
        <version>${spring-boot.version}</version>
        <type>pom</type>
        <scope>import</scope>
        <exclusions>
          <exclusion>
            <artifactId>jna</artifactId>
            <groupId>net.java.dev.jna</groupId>
          </exclusion>
          <exclusion>
            <artifactId>jna-platform</artifactId>
            <groupId>net.java.dev.jna</groupId>
          </exclusion>
        </exclusions>
      </dependency>
      <dependency>
        <groupId>com.orbitz.consul</groupId>
        <artifactId>consul-client</artifactId>
        <version>${consul-client.version}</version>
      </dependency>
      <dependency>
        <groupId>org.jetbrains.pty4j</groupId>
        <artifactId>pty4j</artifactId>
        <version>${pty4j.version}</version>
      </dependency>
      <dependency>
        <groupId>org.projectlombok</groupId>
        <artifactId>lombok</artifactId>
        <version>${lombok.version}</version>
      </dependency>
      <dependency>
        <groupId>net.java.dev.jna</groupId>
        <artifactId>jna</artifactId>
        <version>${jna.version}</version>
      </dependency>
      <dependency>
        <groupId>net.java.dev.jna</groupId>
        <artifactId>jna-platform</artifactId>
        <version>${jna.version}</version>
      </dependency>
    </dependencies>
  </dependencyManagement>
  <repositories>
    <repository>
      <id>huaweicloud-jetbrains-pty4j</id>
      <name>huaweicloud</name>
      <url>https://repo.huaweicloud.com/repository/maven/</url>
    </repository>
  </repositories>
  <build>
    <pluginManagement>
      <plugins>
        <plugin>
          <artifactId>maven-surefire-plugin</artifactId>
          <version>${maven-surefire-plugin.version}</version>
        </plugin>
        <plugin>
          <artifactId>maven-compiler-plugin</artifactId>
          <version>${maven-compiler-plugin.version}</version>
        </plugin>
        <plugin>
          <artifactId>maven-resources-plugin</artifactId>
          <version>${maven-resources-plugin.version}</version>
        </plugin>
        <plugin>
          <groupId>org.springframework.boot</groupId>
          <artifactId>spring-boot-maven-plugin</artifactId>
          <version>${spring-boot.version}</version>
        </plugin>
        <plugin>
          <artifactId>maven-antrun-plugin</artifactId>
          <version>${maven-antrun-plugin.version}</version>
        </plugin>
      </plugins>
    </pluginManagement>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
          <encoding>UTF-8</encoding>
          <optimize>true</optimize>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
